{
  "manifest_version": 1,
  "name": "Ygmailinator",
  "description": "A chrome extension to add gmail shortcuts to yahoo mail.",
  "version": "0.1",
  "permissions": [
    "https://mail.yahoo.com/",
    "https://secure.flickr.com/"
  ],
  "browser_action": {
    "default_icon": "shark.png",
    "default_popup": "popup.html"
  }
}